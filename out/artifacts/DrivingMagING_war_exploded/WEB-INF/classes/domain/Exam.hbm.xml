<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Exam" table="exam">
        <id name="exam_id" column="exam_id">
            <generator class="native"/>
        </id>
        <property name="exam_date"/>
        <property name="exam_subject"/>
        <property name="exam_currPeople"/>
        <property name="exam_maxPeople"/>
        <property name="exam_room"/>
    </class>
</hibernate-mapping>