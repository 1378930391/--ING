<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Examination" table="examination">
        <id name="source_id" column="source_id">
            <generator class="uuid"/>
        </id>
        <properties name="subject1_time"/>
        <properties name="subject1_source"/>
        <properties name="subject2_time"/>
        <properties name="subject2_source"/>
        <properties name="subject3_time"/>
        <properties name="subject3_source"/>
        <properties name="subject4_time"/>
        <properties name="subject4_source"/>
        <!--配置学生成绩表 单向一对一外键关系-->
        <many-to-one name="student" class="domain.Student" column="exam_stu_id"/>
    </class>
</hibernate-mapping>